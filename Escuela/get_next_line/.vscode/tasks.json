{
// Vea https://go.microsoft.com/fwlink/?LinkId=733558 
	// para la documentaci√≥n acerca del formato tasks.json
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "uno",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/a.out"
			],
			"options": {
				"cwd": "/usr/bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"type": "shell",
			"label": "todos",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"${fileDirname}/*.c",
				"-o",
				"${fileDirname}/a.out"
			],
			"options": {
				"cwd": "/usr/bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"type": "shell",
			"label": "get next line",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"${fileDirname}/main.c",
				"${fileDirname}/get_next_line.c",
				"${fileDirname}/../libft/libft.a",
				"-o",
				"${fileDirname}/test",
			],
			"options": {
				"cwd": "/usr/bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}